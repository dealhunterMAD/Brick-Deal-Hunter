# Firebase Cloud Functions Environment Variables
# Copy this file to .env and fill in your values
# NEVER commit .env to git!

# ============================================
# API KEYS
# ============================================

# Rebrickable API Key (get free key at https://rebrickable.com/api/)
REBRICKABLE_API_KEY=your_rebrickable_key_here

# ============================================
# SECURITY
# ============================================

# API key for authenticating admin requests to Cloud Functions
# Generate a secure random string (e.g., using: openssl rand -hex 32)
APP_API_KEY=your_secure_api_key_here
